generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  username String
  name     String
}

model karakter {
  id               Int              @id @default(autoincrement())
  ID_artefak       Int
  ID_charasup      Int
  ID_batu          Int
  Nama_chara       String
  Mat_ascend       String
  Gambar_mat       String
  Lokal_mat        String
  gambar_lokalmat  String
  Elemen           String
  Senjata1         String
  Gambar_Senjata1  String
  Senjata2         String
  Gambar_Senjata2  String
  Senjata3         String
  Gambar_Senjata3  String
  Senjata4         String
  Gambar_Senjata4  String
  artefak          Artefak?          @relation(fields: [ID_artefak], references: [id])
  karakter_support Karakter_Support? @relation(fields: [ID_charasup], references: [id])
  batu_ascend      Batu_ascend?     @relation(fields: [ID_batu], references: [id])
}

model Batu_ascend {
  id              Int       @id @default(autoincrement())
  Silver          String
  Gambar_silver   String
  Fragment        String
  Gambar_fragment String
  Chunk           String
  Gambar_chunk    String
  Gemstone        String
  Gambar_gemstone String
  karakter        karakter[] // No `@relation` attributes here
}

model Karakter_Support {
  id             Int       @id @default(autoincrement())
  Nama_charasup  String
  Foto_charasup  String
  karakter       karakter[] // No `@relation` attributes here
}

model Artefak {
  id            Int       @id @default(autoincrement())
  Nama_artefak  String
  Foto_artefak  String
  karakter      karakter[] // No `@relation` attributes here
}
